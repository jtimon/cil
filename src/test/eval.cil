mode test

import("src/core.cil")

test(eq(btoi(true). atoi(eval_to_str("true")), "eval_to_str() can't turn literal bools strings into 'true'")
test(eq(btoi(false). atoi(eval_to_str("false")), "eval_to_str() can't turn literal bools strings into 'false'")
test(eq(42. atoi(eval_to_str("42")), "eval_to_str() can't turn literal numbers strings into ints")
test(str_eq("aaa". eval_to_str("aaa"), "eval_to_str() can't turn literal strings into identical strings")
